Template → Comandos

Comandos → Comandos Comando
	 | ε
	 
Comando → Declaracao
	| if Cond Declaracao //exemplo: if alunos.get(5)>alunos.get(6) {alunos=5}
	| for NUM Declaracao //exemplo: for 4 {alunos} -> printa 4 vezes os alunos
	| foreach ID 'in' ID  //exemplo: foreach a in alunos ->printa todos alunos
	
Declaracao → '{' ID '=' NUM '}'	
	
Cond → Exp '<' Exp
	| Exp '>' Exp 
	| Exp '<=' Exp 
	| Exp '>=' Exp 
	| Exp '==' Exp  

Exp → NUM
    | ID '.get(' NUM ')'    //o objetivo é fazer tp aluno.get(3) → devolve o terceiro campo de aluno
